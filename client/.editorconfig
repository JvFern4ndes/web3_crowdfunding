# EditorConfig é um arquivo de configuração que ajuda a manter o estilo de codificação consistente.
# Mais informações: http://editorconfig.org

root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[*.md]
trim_trailing_whitespace = false

[*.{js,jsx,ts,tsx}]
indent_style = space
indent_size = 4
